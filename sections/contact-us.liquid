<style>
  @media only screen and (max-width: 600px) {
  .corrporate-btn{
    padding: 20px 10px;
    font-size: 25px;
    width: 100%;
    height: 4rem;
  }
}
</style>

{%- assign social_icons = false -%}

{% if
  settings.social_twitter_link != blank
  or settings.social_facebook_link != blank
  or settings.social_pinterest_link != blank
  or settings.social_instagram_link != blank
  or settings.social_tumblr_link != blank
  or settings.social_snapchat_link != blank
  or settings.social_youtube_link != blank
  or settings.social_vimeo_link != blank
  or template.name == 'article'
  or template.name == 'blog'
%}
  {%- assign social_icons = true -%}
{% endif %}

	<div class="corporate-button-wrapper" style="max-width: 940px; padding-top: 6rem">
  		{% for block in section.blocks %}
        	<div class="">
              
              	<a href="{% if block.settings.url_box != blank %}{{ block.settings.url_box }}{% else %}javascript:void('0'){% endif %}" class="link-block-stripe w-inline-block">
              
              {% if block.settings.title_heading != blank %}
                <div class="corrporate-btn">
                  {{ block.settings.title_heading }}
                </div>
              {% endif %}
              </a>
        	</div>
  		{% endfor %}
        
      </div>
  </div>
  </div>
</div>

              
      </div>
  <div hidden class="map" id="address" data-latitude="{{ section.settings.latitude }}" data-longitude="{{ section.settings.longitude }}">
    
    
 	<div hidden class="contact-div">
      <div hidden class="div-50 colored-box">
        <div hidden class="colored-box-1">
          <div hidden class="colored-box-2">
            {% if section.settings.location != blank %}
            <h3 class="h3-white">{{ section.settings.location }}</h3>
            {% endif %}
            
            <h3 class="h3-white">
              {% if section.settings.email != blank %}
              	{{ section.settings.email }}
              {% endif %}
              {% if section.settings.phone != blank %}
              	<br>{{ section.settings.phone }}
              {% endif %}
            
            </h3>
            {% if social_icons %}
            <div class="social-icons-box stay-connected left">
              {% if settings.social_facebook_link != blank %}
              <a href="{{ settings.social_facebook_link | escape }}" data-ix="show-social-icon-on-hover" class="social-icon w-inline-block">
                <span style="display:none">Social liks</span>{% include 'icon-facebook' %}
                  </a>
              {% endif %}

              {% if settings.social_twitter_link != blank %}
                <a href="{{ settings.social_twitter_link | escape }}" data-ix="show-social-icon-on-hover" class="social-icon w-inline-block">
                  {% include 'icon-twitter' %}
                  <span style="display:none">Social liks</span>
                </a>
              {% endif %}

              {% if settings.social_instagram_link != blank %}
              <a href="{{ settings.social_instagram_link | escape }}" data-ix="show-social-icon-on-hover" class="social-icon w-inline-block">
                {% include 'icon-instagram' %}
                <span style="display:none">Social liks</span>
              </a>
              {% endif %}

              {% if settings.social_pinterest_link != blank %}
                <a href="{{ settings.social_pinterest_link | escape }}" data-ix="show-social-icon-on-hover" class="social-icon w-inline-block">
                  {% include 'icon-pinterest' %}
                  <span style="display:none">Social liks</span>
                </a>
              {% endif %}

              {% if settings.social_tumblr_link != blank %}
                <a class="social-icon w-inline-block" href="{{ settings.social_tumblr_link | escape }}" data-ix="show-social-icon-on-hover" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Tumblr' }}">
                  {% include 'icon-tumblr' %}
                  <span style="display:none">Social liks</span>
                </a>
              {% endif %}

              {% if settings.social_snapchat_link != blank %}
                <a class="ssocial-icon w-inline-block" href="{{ settings.social_snapchat_link | escape }}" data-ix="show-social-icon-on-hover" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Snapchat' }}">
                  <span style="display:none">Social liks</span>
                  {% include 'icon-snapchat' %}
                  
                </a>
              {% endif %}
              {% if settings.social_youtube_link != blank %}
                  <a class="social-icon w-inline-block" href="{{ settings.social_youtube_link | escape }}" data-ix="show-social-icon-on-hover" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'YouTube' }}">
                    <span style="display:none">Social liks</span>
                    {% include 'icon-youtube' %}
                    
                  </a>
              {% endif %}
              {% if settings.social_vimeo_link != blank %}
                  <a class="social-icon w-inline-block" href="{{ settings.social_vimeo_link | escape }}" data-ix="show-social-icon-on-hover" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Vimeo' }}">
                    <span style="display:none">Social liks</span>
                    {% include 'icon-vimeo' %}
                    
                  </a>
              {% endif %}
              {% if template.name == 'blog' or template.name == 'article' %}
                  <a class="social-icon w-inline-block" href="{{ shop.url }}{{ blog.url }}.atom" data-ix="show-social-icon-on-hover" >
                    <span style="display:none">Social liks</span>
                    {% include 'icon-rss' %}
                    
                  </a>
              {% endif %}
            </div>
            {% endif %}
          </div>
        </div>
      </div>
    </div>
