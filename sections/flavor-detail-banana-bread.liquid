          },
          {
            "type": "richtext",
            "id": "text",
            "label": "Description"
          }
        ]
      },
      {
        "type": "text_block_2",
        "name": "bottom content",
        "settings": [
          {
            "type": "text",
            "id": "title",
            "label": "Title"
          },
          {
            "type": "richtext",
            "id": "text",
            "label": "Description"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "banana bread",
        "category": "Text",
        "blocks": [
          {
            "type": "text_block"
          },
          {
            "type": "text_block"
          },
          {
            "type": "text_block"
          },
          {
            "type": "text_block_2"
          },
          {
            "type": "text_block_2"
          },
          {
            "type": "text_block_2"
          }
        ]
      }
    ]
  }
{% endschema %}



