<div class="content-section locations">
<div class="div-container">
<div class="div-block-16">
<div class="div-block-15">

<div class="colored-box auto" style="height: 25px;"></div>
  

<style>
  @media screen and (min-width: 767px){
    .text-padding{
    	padding: 0px 20px;
    } 
  }
</style>
<div class="div-50 _100 _25">
<div class="w-row visit-timing-wrapper">
  {% if section.settings.hide_1 == true %}
<div class="w-col {% if section.settings.hide_2 == false %} w-col-12{% else %}w-col-6{% endif %}">
  <div class="location-box" {% if section.settings.hide_2 == false %} style="max-width:100%;" {% endif %}>
  
    {% if section.settings.hide_2 == false %}
    <div class="w-row">
      <div class="w-col w-col-6">
    {% endif %}
                
                
    {% if section.settings.left_image != blank %}
  <img class="location-image" src="{{ section.settings.left_image | img_url: '600x' }}" alt="CookieDO NYC"/>
 {% endif %}
 <div class="div-50 colored-box" style="max-width:100%;">
  <div class="colored-box-1">
{% if section.settings.left_title != blank %}
  <h2 class="box-heading-bold curly">{{ section.settings.left_title }}</h2>
 {% endif %}

  </div>
  </div>
    {% if section.settings.hide_2 == false %}
        <div class="empty-spacer" style="display:block;"></div>
      </div>
      
      <div class="w-col w-col-6">
        <div class="text-padding">
    {% endif %}
    
    
     {% if section.settings.left_description != blank %}
      <div class="location-description">
      {{ section.settings.left_description }}
      </div>
    {% endif %}
    {% if section.settings.left_location != blank %}
    <div class="location">
    	{{ section.settings.left_location }}
    </div>
    {% endif %}
  {% if section.settings.left_url != blank %}
    <a class="btn view_more" href="{{section.settings.left_url}}">view menu</a>
    {% endif %}
    
    
     {% if section.settings.hide_2 == false %}
        </div>
      </div>
  </div>
    {% endif %}
    
  </div>
</div>
{% endif %}
  {% if section.settings.hide_2 == true %}
<div class="w-col  {% if section.settings.hide_1 == false %}w-col-12{% else %}w-col-6{% endif %}">
  <div class="location-box"  {% if section.settings.hide_1 == false %} style="max-width:100%;" {% endif %}>
 
    {% if section.settings.hide_1 == false %}
    <div class="w-row">
      <div class="w-col w-col-6">
    {% endif %}
    
    
    
    {% if section.settings.right_image != blank %}
  <img class="location-image" src="{{ section.settings.right_image | img_url: '600x' }}" alt="CookieDO NYC"/>
 {% endif %}
  <div class="div-50 colored-box" style="max-width:100%;">
  <div class="colored-box-1">

  {% if section.settings.right_title != blank %}
  <h2 class="box-heading-bold curly">{{ section.settings.right_title }}</h2>
 {% endif %}

  </div>
  </div>
        
        {% if section.settings.hide_1 == false %}
        <div class="empty-spacer" style="display:block;"></div>
      </div>
      <div class="w-col w-col-6">
        <div class="text-padding">
    {% endif %}
        
  
 <div class="location-description">
  {{ section.settings.right_description }}
  </div>
     {% if section.settings.right_location != blank %}
    <div class="location">
    	{{ section.settings.right_location }}
    </div>
    {% endif %}
  {% if section.settings.right_url != blank %}
    <a class="btn view_more" href="{{section.settings.right_url}}">view menu</a>
    {% endif %}
        </div>
</div>
  </div>
  {% endif %}
    
     {% if section.settings.hide_1 == false %}
      </div>
  </div>
    {% endif %}
    
    
</div>
</div>


  </div>
</div>
</div>
  
{%- assign social_icons = false -%}

{% if
  settings.social_twitter_link != blank
  or settings.social_facebook_link != blank
  or settings.social_pinterest_link != blank
  or settings.social_instagram_link != blank
  or settings.social_tumblr_link != blank
  or settings.social_snapchat_link != blank
  or settings.social_youtube_link != blank
  or settings.social_vimeo_link != blank
  or template.name == 'article'
  or template.name == 'blog'
%}
  {%- assign social_icons = true -%}
{% endif %}

	<div class="div-block-21">
  		{% for block in section.blocks %}
        	<div class="column-center w-col w-col-4">
              {% if block.settings.url_box != blank %}
              	<a href="{{ block.settings.url_box }}" class="link-block-stripe w-inline-block">
              {% else %}
              	<a href="javascript:void('0')" class="link-block-stripe w-inline-block">
              {% endif %}
              {% if block.settings.title_heading != blank %}
                <div class="link-box">
                  {{ block.settings.title_heading }}
                </div>
              {% endif %}
              </a>
        	</div>
  		{% endfor %}
              
      </div>
<div class="contact-row"></div>
  </div>
{% schema %}
  {
    "name": "Visit Timing",
    "settings": [ 
			{
              "type": "checkbox",
              "id": "hide_1",
              "label": "Enable / Disable",
			  "default": true
            },
			{
              "type": "text",
              "id": "left_title",
              "label": "Title"
            },
			{
              "type": "image_picker",
              "id": "left_image",
              "label": "Image"
            },
			{
              "type": "html",
              "id": "left_description",
              "label": "Description"
            },
			{
              "type": "html",
              "id": "left_location",
              "label": "Location"
            },
			{
              "type": "url",
              "id": "left_url",
              "label": "URL"
            },
			{
              "type": "checkbox",
              "id": "hide_2",
              "label": "Enable / Disable",
			  "default": true
            },
			{
              "type": "text",
              "id": "right_title",
              "label": "Title"
            },
			{
              "type": "image_picker",
              "id": "right_image",
              "label": "Image"
            },
			{
              "type": "html",
              "id": "right_description",
              "label": "Description"
            },
			{
              "type": "html",
              "id": "right_location",
              "label": "Location"
            },
			{
              "type": "url",
              "id": "right_url",
              "label": "URL"
            }
		],
        "blocks":[
            {
            "type":"location-hours",
            "name":"Additional content",
            "settings":[
                        {
                            "type"  : "html",
                            "id"    : "title_heading",
                            "label" : "Title"
                        },
                        {
                            "type"  : "text",
                            "id"    : "url_box",
                            "label" : "URL"
                        }
                    ]
            }
        ],
	"presets": [
      {
        "name": "Visit Timing",
        "category": "Text"
      }
    ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}
