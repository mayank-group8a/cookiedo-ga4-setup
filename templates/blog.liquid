              })
            jQuery(document).ready(function() {
              console.log('working');
              
              
              jQuery('.blog-pagination a').on('click', function(e){
                e.preventDefault();
                jQuery('.blog-pagination li').removeClass('active');
                var active_class = jQuery(this).parent('li').attr('class');
                jQuery('.'+active_class).addClass('active');
              	var page_id = jQuery(this).attr('href');
                jQuery(page_id).fadeIn().siblings('.blog-paginate-container').hide();
                
                $('html, body').animate({scrollTop: $('.all-blog-header').offset().top -50 }, 'slow');

                return false;
              });
              
            })
          </script>