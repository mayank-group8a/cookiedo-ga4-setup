<style>
  .fontstyle3 img{
  	max-width:31.33%
      margin:10px 1%;
  }

  .cat-inner-wrapper{
    display: block;
    border: 2px solid #231f20;
    padding: 25px 25px 15px 25px;
    height: 100%;
    margin-bottom:25px;
}
.image-thumbnail{
  	display: block;
    padding-top: 100%;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
}
  .cat-content{
  	padding-top:15px;
    display:block;
    text-align:left;
  }
   .cat-content .btn-outline{
      border: 1px solid #000;
   	  display: inline-block;
      transition: all 0.5;
      font-size: 18px;
      padding: 7px 15px;
     font-weight: 300;
}
  
  .cat-content h4{
    display: block;
    text-align: center;
    font-size: 30px;
    padding-top: 10px;
        line-height: 35px;
}
  
.cat-content p{
    display: block;
    text-align: center;
    font-size: 18px;
    padding-top: 10px;
}
  .featured-products{
  	padding-bottom:20px;
  }
  
  .blog-col{margin-bottom:20px;}
  
  @media screen and (min-width: 768px){
  
    .featured-products{
    	display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display:         flex;
  flex-wrap: wrap;
    }
  }
  .blog-pagination{ margin-top: 40px; width:100%; padding:0px; list-style:none; display:flex; justify-content: center;}
.blog-pagination a{
    padding: 4px 13px;
    display: block;
}
  .blog-pagination li{
  	border-top:1px solid #eee;
    border-bottom:1px solid #eee;
    border-left:1px solid #eee;
  }
  .blog-pagination li:last-child{
  	border-right:1px solid #eee;
  }
  .blog-pagination li.active a{
  	    color: #000;
    background: #eee;
  }
</style>

